{
  "name": "opcao-project",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:serve:before": "node ./src/app/config/script.js",
    "ionic:build:before": "node ./src/app/config/script.js",
    "run-android": "rm -rf platforms/* && cross-env ENV=prod ionic cordova run android",
    "emulate-android": "rm -rf platforms/* && cross-env ENV=prod ionic cordova emulate android",
    "build-android": "rm -rf platforms/* && cross-env ENV=prod ionic cordova build android"
  },
  "config": {
    "ionic_copy": "./config/copy.config.js",
    "ionic_sass": "./config/sass.config.js"
  },
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/core": "11.0.5",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@ionic-native/camera": "^4.20.0",
    "@ionic-native/core": "^4.7.0",
    "@ionic-native/file": "^5.0.0",
    "@ionic-native/file-transfer": "^5.5.1",
    "@ionic-native/ionic-webview": "^5.0.0",
    "@ionic-native/network": "^4.9.1",
    "@ionic-native/splash-screen": "4.6.0",
    "@ionic-native/status-bar": "4.6.0",
    "@ionic/pro": "^1.0.20",
    "@ionic/storage": "^2.1.3",
    "cordova-android": "7.1.4",
    "cordova-browser": "^5.0.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-add-swift-support": "^1.7.2",
    "cordova-plugin-camera": "4.0.3",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-file": "6.0.1",
    "cordova-plugin-file-transfer": "1.7.1",
    "cordova-plugin-ionic": "^4.1.7",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^3.1.2",
    "cordova-plugin-network-information": "^2.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sqlite-storage": "3.0.0",
    "datepicker-for-ionic": "^1.1.7",
    "font-awesome": "^4.7.0",
    "ionic-angular": "3.9.2",
    "ionic-img-viewer": "^2.9.0",
    "ionicons": "3.0.0",
    "rxjs": "5.5.8",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^3.2.3",
    "cross-env": "^5.2.0",
    "typescript": "^2.8.3"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-ionic": {
        "APP_ID": "F2C642A4",
        "CHANNEL_NAME": "Production",
        "UPDATE_METHOD": "background|auto|none",
        "MAX_STORE": "2",
        "WARN_DEBUG": "true",
        "UPDATE_API": "https://api.ionicjs.com"
      },
      "cordova-plugin-file": {},
      "cordova-plugin-file-transfer": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-camera": {}
    },
    "platforms": [
      "browser",
      "android",
      "ios"
    ]
  }
